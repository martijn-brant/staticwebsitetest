{
    "routes": [
      {
        "route": "/secret.html",
        "allowedRoles": ["matties"]
      },
      {
        "route": "/login",
        "serve": "/.auth/login/github"
      },
      {
        "route": "/logout",
        "serve": "/.auth/logout"
      }
    ],
    "platformErrorOverrides": [

      {
        "errorType": "Unauthenticated",
        "statusCode": "301",
        "serve": "/login"
      },
      {
        "errorType": "Unauthorized_MissingRoles",
        "statusCode": "301",
        "serve": "/notallowed.html"
      },
      {
        "errorType": "Unauthorized",
        "statusCode": "301",
        "serve": "/notallowed.html"
      }
    ]
  }